package main

import "fmt"

func main() {
  var n1, n2,n3 float64
  fmt.Println("Digite a primeira nota:")
  fmt.Scanf("%f", &n1)
  fmt.Println("Digite a segunda nota:")
  fmt.Scanf("%f", &n2)
  fmt.Println("Digite a terceira nota:")
  fmt.Scanf("%f", &n3)
  
  m := (n1 + n2 + n3) / 3
  fmt.Printf("A sua média é: %.2f\n", m)
   if m >= 6.00{
     fmt.Println("APROVADO")
 } else {
     fmt.Println("REPROVADO")
 }
}
